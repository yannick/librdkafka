@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

if(@WITH_ZLIB@)
  find_dependency(ZLIB)
endif()

if(@WITH_SSL@)
  if(@WITH_BUNDLED_SSL@)
    MESSAGE(STATUS "WITH_BUNDLED_SSL was picket up")
    find_dependency(OpenSSL)
  else()
  	MESSAGE(STATUS "WITH_BUNDLED_SSL was NOT picket up")
    find_dependency(OpenSSL)
  endif()
else(@WITH_SSL@)
  MESSAGE(STATUS "NO OpenSSL was picket up")
endif()

find_dependency(Threads)

include("${CMAKE_CURRENT_LIST_DIR}/@targets_export_name@.cmake")
check_required_components("@PROJECT_NAME@")
